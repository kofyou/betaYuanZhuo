<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/buttonbackground"
        android:gravity="center"
        android:text="返回"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scrollView2" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="427dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/make">

        <LinearLayout
            android:id="@+id/line"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.218"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/make">

            <android.support.constraint.ConstraintLayout
                android:id="@+id/constraintLayout3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="@+id/line"
                app:layout_constraintStart_toStartOf="@+id/line"
                app:layout_constraintTop_toTopOf="@+id/line">

                <TextView
                    android:id="@+id/xms"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:text="徐明盛"
                    app:layout_constraintStart_toEndOf="@+id/imageView"
                    app:layout_constraintTop_toTopOf="@+id/imageView" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="0dp"
                    android:layout_height="28dp"
                    android:text="项目组长，后端工程师"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/imageView"
                    app:layout_constraintTop_toBottomOf="@+id/xms" />

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="52dp"
                    android:layout_height="44dp"
                    app:layout_constraintStart_toStartOf="@+id/constraintLayout3"
                    app:layout_constraintTop_toTopOf="@+id/constraintLayout3"
                    app:srcCompat="@mipmap/xms" />

            </android.support.constraint.ConstraintLayout>

            <android.support.constraint.ConstraintLayout
                android:id="@+id/constraintLayout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                app:layout_constraintEnd_toEndOf="@+id/line"
                app:layout_constraintStart_toStartOf="@+id/line"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout3">

                <ImageView
                    android:id="@+id/fhtimage"
                    android:layout_width="52dp"
                    android:layout_height="44dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@mipmap/fht" />

                <TextView
                    android:id="@+id/fhtintroduction"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="核心开发者，后端工程师，具有多次项目经验"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toEndOf="@+id/fhtimage"
                    app:layout_constraintTop_toBottomOf="@+id/fhtname" />

                <TextView
                    android:id="@+id/fhtname"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="傅海涛"
                    android:textSize="15sp"
                    app:layout_constraintStart_toEndOf="@+id/fhtimage"
                    app:layout_constraintTop_toTopOf="@+id/fhtimage" />

            </android.support.constraint.ConstraintLayout>

            <android.support.constraint.ConstraintLayout
                android:id="@+id/constraintLayout4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                app:layout_constraintEnd_toEndOf="@+id/line"
                app:layout_constraintStart_toStartOf="@+id/line"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

                <TextView
                    android:id="@+id/wt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="王龙涛"
                    app:layout_constraintStart_toEndOf="@+id/imageView2"
                    app:layout_constraintTop_toTopOf="@+id/imageView2" />

                <TextView
                    android:id="@+id/wltint"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="前端工程师"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toEndOf="@+id/imageView2"
                    app:layout_constraintTop_toBottomOf="@+id/wt" />

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="52dp"
                    android:layout_height="44dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@mipmap/wlt" />

            </android.support.constraint.ConstraintLayout>

            <android.support.constraint.ConstraintLayout
                android:id="@+id/constraintLayout6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                app:layout_constraintEnd_toEndOf="@+id/line"
                app:layout_constraintStart_toStartOf="@+id/line"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout4">

                <TextView
                    android:id="@+id/wlw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="魏璐炜"
                    app:layout_constraintStart_toEndOf="@+id/imageView3"
                    app:layout_constraintTop_toTopOf="@+id/imageView3" />

                <TextView
                    android:id="@+id/wlwint"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="前端工程师"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/imageView3"
                    app:layout_constraintTop_toBottomOf="@+id/wlw" />

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="52dp"
                    android:layout_height="44dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@mipmap/wlw" />

            </android.support.constraint.ConstraintLayout>

            <android.support.constraint.ConstraintLayout
                android:id="@+id/constraintLayout9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                app:layout_constraintEnd_toEndOf="@+id/line"
                app:layout_constraintStart_toStartOf="@+id/line"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout8">

                <ImageView
                    android:id="@+id/nkwimage"
                    android:layout_width="52dp"
                    android:layout_height="44dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@mipmap/nkw" />

                <TextView
                    android:id="@+id/nkwintroduction"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="前端工程师"
                    app:layout_constraintStart_toEndOf="@+id/nkwimage"
                    app:layout_constraintTop_toBottomOf="@+id/nkwname" />

                <TextView
                    android:id="@+id/nkwname"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="牛康文"
                    android:textSize="15sp"
                    app:layout_constraintStart_toEndOf="@+id/nkwimage"
                    app:layout_constraintTop_toTopOf="@+id/nkwimage" />

            </android.support.constraint.ConstraintLayout>

            <android.support.constraint.ConstraintLayout
                android:id="@+id/constraintLayout8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                app:layout_constraintEnd_toEndOf="@+id/line"
                app:layout_constraintStart_toStartOf="@+id/line"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout7">

                <ImageView
                    android:id="@+id/hjximage"
                    android:layout_width="52dp"
                    android:layout_height="44dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@mipmap/hjx" />

                <TextView
                    android:id="@+id/hjxintroduction"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="前端工程师"
                    app:layout_constraintStart_toEndOf="@+id/hjximage"
                    app:layout_constraintTop_toBottomOf="@+id/hjxname" />

                <TextView
                    android:id="@+id/hjxname"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="黄家雄"
                    android:textSize="15sp"
                    app:layout_constraintStart_toEndOf="@+id/hjximage"
                    app:layout_constraintTop_toTopOf="@+id/hjximage" />

            </android.support.constraint.ConstraintLayout>

            <android.support.constraint.ConstraintLayout
                android:id="@+id/constraintLayout7"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:layout_marginTop="30dp"
                app:layout_constraintEnd_toEndOf="@+id/line"
                app:layout_constraintStart_toStartOf="@+id/line"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout6">

                <ImageView
                    android:id="@+id/xbimage"
                    android:layout_width="52dp"
                    android:layout_height="44dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@mipmap/xb" />

                <TextView
                    android:id="@+id/xbintroduction"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="测试员"
                    app:layout_constraintBottom_toBottomOf="@+id/xbimage"
                    app:layout_constraintStart_toEndOf="@+id/xbimage"
                    app:layout_constraintTop_toBottomOf="@+id/xbname" />

                <TextView
                    android:id="@+id/xbname"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="许斌"
                    android:textSize="15sp"
                    app:layout_constraintStart_toEndOf="@+id/xbimage"
                    app:layout_constraintTop_toTopOf="@+id/xbimage" />

            </android.support.constraint.ConstraintLayout>

        </LinearLayout>
    </ScrollView>

    <TextView
        android:id="@+id/make"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="制作团队"
        android:textColor="#0F0F0F"
        android:textSize="20sp"
        android:textStyle="bold" />


</android.support.constraint.ConstraintLayout>